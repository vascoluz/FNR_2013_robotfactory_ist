void setup() {
  pinMode(4,OUTPUT); 
  pinMode(5,OUTPUT);
  pinMode(6,OUTPUT); 
  pinMode(7,OUTPUT);
  pinMode(8,OUTPUT); 
  pinMode(9,OUTPUT);
  pinMode(10,OUTPUT); 
  pinMode(11,OUTPUT); 
}

void loop() {
  digitalWrite(4,1);
  digitalWrite(5,0);
  
  digitalWrite(6,1);
  digitalWrite(7,0);
  
  digitalWrite(8,1);
  digitalWrite(9,0);
  
  digitalWrite(10,1);
  digitalWrite(11,0);
}
